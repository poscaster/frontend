{
  "name": "poscaster",
  "version": "0.0.0",
  "repository": "git://github.com/poscaster/frontend.git",
  "scripts": {
    "lint": "eslint --ext js,jsx src specs",
    "build": "webpack --config webpack.prod.config.js",
    "dev:build": "webpack --config webpack.dev.config.js",
    "dev:serve": "webpack-dev-server --config webpack.dev.config.js",
    "test": "env BABEL_DISABLE_CACHE=1 NODE_ENV=test babel-node node_modules/.bin/babel-istanbul cover _mocha -- -R spec --require ./specs/specs_helper.js \"specs/**/*@(.js|.jsx)\"",
    "test:watch": "env BABEL_DISABLE_CACHE=1 NODE_ENV=test mocha --compilers js:babel-register --require babel-polyfill --require ./specs/specs_helper.js \"specs/**/*@(.js|.jsx)\" --watch"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "engines": {
    "node": "~>6"
  },
  "dependencies": {
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "js-cookie": "^2.1.4",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.8",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^5.0.4",
    "react-router": "^4.0.0",
    "react-router-redux": "^4.0.6",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.0.1",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-istanbul": "^0.12.0",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.16.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.0",
    "chai-immutable": "^1.6.0",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.28.0",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.4.1",
    "eslint": "^3.8.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.4.1",
    "file-loader": "^0.11.0",
    "ignore-styles": "^5.0.1",
    "image-webpack-loader": "^3.0.0",
    "jsdom": "^10.0.0",
    "mocha": "^3.1.2",
    "nock": "^9.0.2",
    "node-sass": "^4.5.2",
    "postcss-loader": "^2.0.5",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-mock-store": "^1.2.1",
    "sass-loader": "^6.0.3",
    "sinon": "^2.0.0",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.0.0",
    "webpack-dev-server": "^2.4.0"
  }
}
